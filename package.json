  "license": "MIT",
  "repository": "",
  "keywords": [
    "typescript"
  ],
  "source": "src/index.ts",
  "main": "./jackett-definitions.js",
  "module": "./jackett-definitions.m.js",
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "microbundle build --target node --format cjs,es -o dist",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "23.3.10",
    "@types/js-yaml": "3.11.2",
    "@types/node": "10.12.10",
    "@types/prettier": "^1.15.2",
    "@types/rimraf": "^2.0.2",
    "jest": "23.6.0",
    "js-yaml": "3.12.0",
    "microbundle": "0.7.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "semantic-release": "15.12.3",
    "travis-deploy-once": "5.0.9",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-xo": "0.10.0",
    "typedoc": "0.13.0",
    "typescript": "3.1.6"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
