    "@types/node": "12.0.4",
    "@types/prettier": "1.16.4",
    "del": "4.1.1",
    "del-cli": "2.0.0",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "js-yaml": "3.13.1",
    "lodash": "4.17.11",
    "prettier": "1.17.1",
    "semantic-release": "15.13.12",
    "ts-jest": "24.0.2",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
