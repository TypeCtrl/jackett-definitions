{
  "name": "@ctrl/tracker-definitions",
  "version": "0.0.0-placeholder",
  "description": "API definitions for your favorite tracker",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/jackett-definitions",
  "keywords": [
    "Torznab",
    "jackett",
    "cardigann",
    "torrent",
    "tracker"
  ],
  "main": "./index.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "scripts": {
    "update": "ts-node scripts/build-definitions.ts",
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json && cp -r src/json dist/json",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "24.0.13",
    "@types/js-yaml": "3.12.1",
    "@types/lodash": "4.14.130",
    "@types/node": "12.0.2",
    "@types/prettier": "1.16.4",
    "@types/rimraf": "2.0.2",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "js-yaml": "3.13.1",
    "lodash": "4.17.11",
    "prettier": "1.17.1",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.12",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-xo": "0.16.0",
    "typescript": "3.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
